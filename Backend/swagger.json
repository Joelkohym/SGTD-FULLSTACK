    "/login": {
      "post": {
        "summary": "User Login",
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                },
                "required": ["email", "password"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "email": {
                        "type": "string"
                      },
                      "password": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Invalid credentials, please try again.. >>> return render_template('login.html', msg=msg), 401",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "Invalid credentials, please try again.. >>> return render_template('login.html', msg=msg), 401": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "Log in failed, please contact admin. >>> return render_template('login.html', msg=msg), 403",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "Log in failed, please contact admin. >>> return render_template('login.html', msg=msg), 403": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    },
    "/api/table_pull": {
      "post": {
        "summary": "delete_all_rows_table_view(session['gc']) & PULL_vessel_due_to_arrive(PULL VDA from SGTD) >>> return redirect(url_for('table_view_request', imo=user_vessel_imo)).",
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "imo": {
                    "type": "string"
                  }
                },
                "required": ["imo"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Pulls IMO from MPA and SGTD and displays it on yyymmddhhmmssmytable.html",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "user_vessel_imo": {
                        "type": "integer"
                      }
                    }
                  }
                }
              }
            }
          },
          "303": {
            "description": "Pulls IMO from MPA and SGTD and displays it on yyymmddhhmmssmytable.html >>> redirect to table_view_request/<imo>",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "user_vessel_imo": {
                        "type": "integer"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid IMO. Please ensure at IMO is valid. >>> redirect Login Page, 301",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "user_vessel_imo": {
                        "type": "integer"
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "TABLE_PULL g.user is not valid >>> redirect Login Page, 401",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "user_vessel_imo": {
                        "type": "integer"
                      }
                    }
                  }
                }
              }
            }
          },
          "405": {
            "description": "Wrong method, only POST Method allowed, 3405",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "user_vessel_imo": {
                        "type": "integer"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
