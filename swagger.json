{
  "openapi": "3.0.0",
  "info": {
    "title": "SGTD Vessel Query Swagger",
    "description": "API for Vessel Queries",
    "version": "1.0.0"
  },
  "tags": [
    {
      "name": "GET Endpoints"
    },
    {
      "name": "POST Endpoints"
    }
  ],
  "paths": {
    "/table_view": {
      "get": {
        "summary": "return render_template('table_view.html', email=email)",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "/templates/table_view.html": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["GET Endpoints"]
      }
    },
    "/table_view_request/{imo}": {
      "get": {
        "summary": "get_data_from_MPA_Vessel_Arrival_Declaration(imo_list) & get_data_from_vessel_due_to_arrive_and_depart() & merge_arrivedepart_declaration_df(filtered_df_before, Declaration_df) >>> return render_template('table_view.html')",
        "parameters": [
          {
            "name": "imo",
            "in": "path",
            "description": "IMO number(s) as a comma-separated list",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "render template /templates/table_view.html": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["GET Endpoints"]
      }
    },
    "/logout": {
      "get": {
        "summary": "Logout",
        "responses": {
          "302": {
            "description": "Redirect to login page"
          }
        },
        "tags": ["GET Endpoints"]
      }
    },
    "/vessel_request/{msg}": {
      "get": {
        "summary": "return render_template('vessel_request.html', msg=msg, email=email)",
        "parameters": [
          {
            "name": "msg",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful vessel request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "render template /templates/vessel_request.html": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["GET Endpoints"]
      }
    },
    "/api/vessel_map": {
      "get": {
        "summary": "get_map_data(session['gc']) = df1 & display_map(df1) >>> return render_template(display_data[1])",
        "responses": {
          "200": {
            "description": "Successful vessel map request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "render template /templates/yyyymmddhhmmssmymap": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["GET Endpoints"]
      }
    },
    "/api/vessel_due_to_arrive_db/receive/{email_url}": {
      "post": {
        "summary": "Receive Vessel Due to Arrive",
        "parameters": [
          {
            "name": "email_url",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "payload": {
                    "type": "array"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "receive vessel_due_to_arrive Data Element from SGTD json": {
                      "type": "json"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    },
    "/api/pilotage_service_db/receive/{email_url}": {
      "post": {
        "summary": "Receive Pilotage Service",
        "parameters": [
          {
            "name": "email_url",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "payload": {
                    "type": "array"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "receive pilotage_service Data Element from SGTD json": {
                      "type": "json"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    },
    "/api/vessel_current_position_db/receive/{email_url}": {
      "post": {
        "summary": "Receive Vessel Current Position",
        "parameters": [
          {
            "name": "email_url",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "payload": {
                    "type": "array"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "receive vessel_current_position Data Element from SGTD json": {
                      "type": "json"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    },
    "/api/vessel_movement_db/receive/{email_url}": {
      "post": {
        "summary": "Receive Vessel Movement",
        "parameters": [
          {
            "name": "email_url",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "payload": {
                    "type": "array"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "receive vessel_movement Element from SGTD json": {
                      "type": "json"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    },
    "/api/others/receive/{email_url}": {
      "post": {
        "summary": "Receive Others",
        "parameters": [
          {
            "name": "email_url",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "payload": {
                    "type": "array"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "receive any other Data Element from SGTD json": {
                      "type": "json"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    },
    "/login": {
      "post": {
        "summary": "User Login",
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                },
                "required": ["email", "password"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "email": {
                        "type": "string"
                      },
                      "password": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    },
    "/api/table_pull": {
      "post": {
        "summary": "delete_all_rows_table_view(session['gc']) & PULL_vessel_due_to_arrive(PULL VDA from SGTD) >>> return redirect(url_for('table_view_request', imo=user_vessel_imo)).",
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "imo": {
                    "type": "string"
                  }
                },
                "required": ["imo"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Pulls IMO from MPA and SGTD and displays it on yyymmddhhmmssmytable.html",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "user_vessel_imo": {
                        "type": "integer"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    },
    "/register": {
      "post": {
        "summary": "User Registration",
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                },
                "required": ["email", "password"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful registration",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    },
    "/api/vessel": {
      "post": {
        "summary": "delete_all_rows_vessel_location(session['gc']) & PULL_GET_VCP_VDA_MPA >>> return redirect(url_for('Vessel_map'))",
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "vessel_imo": {
                    "type": "string"
                  }
                },
                "required": ["vessel_imo"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful vessel data pull",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "PULL and GET VCP & VDA from SGTD and MPA redirects to /api/vessel_map": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["POST Endpoints"]
      }
    }
  }
}
